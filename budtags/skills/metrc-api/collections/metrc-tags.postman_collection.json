{
  "info": {
    "name": "Metrc Tags API v2",
    "description": "Metrc API v2 endpoints for Tags",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "GetAvailablePlantTags",
      "request": {
        "method": "GET",
        "header": null,
        "url": {
          "host": [
            "{{Metrc.api.server}}/tags/v2"
          ],
          "path": [
            "plant",
            "available"
          ],
          "query": [
            {
              "key": "licenseNumber",
              "value": "{{licenseNumber}}"
            }
          ]
        }
      }
    },
    {
      "name": "GetAvailablePackageTags",
      "request": {
        "method": "GET",
        "header": null,
        "url": {
          "host": [
            "{{Metrc.api.server}}/tags/v2"
          ],
          "path": [
            "package",
            "available"
          ],
          "query": [
            {
              "key": "licenseNumber",
              "value": "{{licenseNumber}}"
            }
          ]
        }
      }
    },
    {
      "name": "GetStagedTags",
      "request": {
        "method": "GET",
        "header": null,
        "url": {
          "host": [
            "{{Metrc.api.server}}/tags/v2"
          ],
          "path": [
            "staged"
          ],
          "query": [
            {
              "key": "licenseNumber",
              "value": "{{licenseNumber}}"
            }
          ]
        }
      }
    }
  ]
}