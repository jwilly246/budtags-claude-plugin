{
  "info": {
    "name": "Metrc PatientsStatus API v2",
    "description": "Metrc API v2 endpoints for PatientsStatus",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "GetStatusesByPatient",
      "request": {
        "method": "GET",
        "header": null,
        "url": {
          "host": [
            "{{Metrc.api.server}}/patients/v2"
          ],
          "path": [
            "statuses",
            "{{patientLicenseNumber}}"
          ],
          "query": [
            {
              "key": "licenseNumber",
              "value": "{{licenseNumber}}"
            }
          ]
        }
      }
    }
  ]
}